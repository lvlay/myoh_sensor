import("//build/ohos.gni")
import("//build/test.gni")
import("./../../../../audio.gni")

module_output_path = "drivers_peripheral_audio/drivers_peripheral_audio"

ohos_fuzztest("EffectModelFuzzTest") {
  module_out_path = module_output_path
  fuzz_config_file = "../effectmodel_fuzzer"

  sources = [ "effectmodel_fuzzer.cpp" ]
  include_dirs = []
  external_deps = [
    "drivers_interface_audio:libeffect_proxy_1.0",
    "hdf_core:libhdf_utils",
  ]
  if (defined(ohos_lite)) {
    external_deps += [ "hilog_lite:hilog_share" ]
  } else {
    external_deps += [ "hilog:libhilog" ]
  }
  if (enable_c_utils) {
    external_deps += [ "c_utils:utils" ]
  }
}
