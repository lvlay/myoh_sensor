import(
    "//drivers/peripheral/distributed_camera/hdi_service/test/fuzztest/dcamera_fuzz.gni")

##############################fuzztest##########################################
ohos_fuzztest("DcameraIsStreamsSupportedFuzzTest") {
  module_out_path = "${fuzz_test_output_path}/dcameraisstreamssupported"

  fuzz_config_file = "${distributedcamera_hdf_path}/hdi_service/test/fuzztest/dcameraisstreamssupported_fuzzer"

  include_dirs += [ "${distributedcamera_hdf_path}/hdi_service/test/fuzztest/dcameraisstreamssupported_fuzzer" ]

  sources = [ "dcameraisstreamssupported_fuzzer.cpp" ]

  deps = dcamera_deps

  defines += [ "DH_LOG_TAG=\"DcameraIsStreamsSupportedFuzzTest\"" ]

  external_deps = dcamera_external_deps
}

###############################################################################
group("fuzztest") {
  testonly = true
  deps = [ ":DcameraIsStreamsSupportedFuzzTest" ]
}
